<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.zhangying.po.Performance" table="performance">
		<id column="id" name="id" type="long">
			<generator class="native"/>
		</id>
		<property name="content" length="1000"  type="string"/>
		<property name="score" length="50" type="float"/>
		<property name="addmoney" length="50" type="float" />
		<property name="createtime" length="23" type="timestamp"/>
	</class>
</hibernate-mapping>
